{
    "name": "ente-web",
    "version": "1.0.0",
    "private": true,
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build:photos": "turbo run build && turbo run export",
        "build:auth": "turbo run build --filter=auth",
        "dev:photos": "turbo run dev --filter=photos",
        "dev:auth": "turbo run dev --filter=auth",
        "lint": "turbo run lint",
        "albums": "turbo run albums",
        "export": "turbo run export",
        "build-analyze": "ANALYZE=true next build",
        "start": "turbo run start",
        "prepare": "husky install"
    },
    "dependencies": {},
    "devDependencies": {
        "eslint": "^8.28.0",
        "husky": "^7.0.1",
        "lint-staged": "^13.2.3",
        "prettier": "2.3.2",
        "turbo": "^1.10.3",
        "typescript": "5.1.3",
        "@ente/eslint-config": "*",
        "@typescript-eslint/parser": "^5.59.2"
    },
    "lint-staged": {
        "apps/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write --ignore-unknown"
        ],
        "packages/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write --ignore-unknown"
        ],
        "**/*.{json,css,scss,md,html,yml,yaml}": [
            "prettier --write --ignore-unknown"
        ]
    }
}
